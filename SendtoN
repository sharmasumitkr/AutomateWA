import pywhatkit
#num='+919717028697'
hri=18
minu=4
def increasemin():
    global hri, minu
    if(minu==59):
        if (hri==23):
            hri=0
            minu=0
        else:
            hri=hri+1
            minu=0
    minu=minu+1

def main():
    for i in range(64):
        increasemin()
        pywhatkit.sendwhatmsg(num, 'test5')

if __name__='__main__':
    main()
